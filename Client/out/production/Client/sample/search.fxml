<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Search">
   <children>
      <TextField fx:id="txtmoviename" layoutX="161.0" layoutY="77.0" prefHeight="31.0" prefWidth="280.0" promptText="Enter Movie Name" />
      <Button layoutX="161.0" layoutY="131.0" mnemonicParsing="false" onAction="#searchButton" text="Search">
         <font>
            <Font name="System Bold Italic" size="17.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="355.0" mnemonicParsing="false" onAction="#back" text="BACK">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Button>
      <Label fx:id="result" layoutX="161.0" layoutY="200.0" prefHeight="39.0" prefWidth="427.0">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Label>
      <Button fx:id="buyticket" layoutX="161.0" layoutY="258.0" mnemonicParsing="false" onAction="#action" prefHeight="31.0" prefWidth="114.0" text="BUY TICKETS">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
